class Solution(object):
    def reverseList(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        if not head:
            return None
        if not head.next:
            return head
        p = self.reverseList(head.next)
        head.next.next = head
        head.next = None
        return p
    
#iterative

def reverseList(head):
    prev = None
    curr = head
    
    while curr:
        temp = curr.next
        curr.next = prev
        prev = curr
        curr = temp
     return prev
     
 def reverseList(head):
    node = Nond
    while head:
        nxt = head.next
        head.next = node
        node = head
        head = nxt
    return node
