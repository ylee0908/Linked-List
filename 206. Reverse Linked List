class Solution(object):
    def reverseList(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        if not head:
            return None
        if not head.next:
            return head
        p = self.reverseList(head.next)
        head.next.next = head
        head.next = None
        return p
    
#iterative

def reverseList(head):
     prev = None
        
        while head:
            nxt = head.next
            cur = head
            cur.next = prev
            head = nxt
            prev = cur
        return prev
     
 
